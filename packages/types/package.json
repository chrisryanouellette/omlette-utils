{
  "name": "@ouellettec/utils-types",
  "version": "1.0.0",
  "license": "MIT",
  "main": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "yarn build --watch",
    "build": "yarn build:code && yarn build:post",
    "build:code": "tsc --project tsconfig.build.json",
    "build:post": "prettier ./dist/**/*.d.ts --write",
    "deploy": "yarn publish --access public",
    "ci": "yarn lint",
    "lint": "yarn lint:tsc",
    "lint:tsc": "yarn tsc --project ./tsconfig.ci.json"
  }
}
